  "devDependencies": {
    "@wdio/allure-reporter": "^9.0.4",
    "@wdio/cli": "^9.0.7",
    "@wdio/cucumber-framework": "^9.0.6",
    "@wdio/local-runner": "^9.0.7",
    "@wdio/spec-reporter": "^9.0.7",
    "github-actions-ctrf": "^0.0.37",
    "supertest": "^7.0.0",
    "wdio-wait-for": "^3.0.11"
  },
  "dependencies": {
